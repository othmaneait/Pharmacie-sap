<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.Customers"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<List
          id="custumorsList"
			headerText="{i18n>custumorsList}"
		class="sapUiResponsiveMargin"
		width="auto"
       items="{
         path : 'customer>/Customers'
        
      }">
         <headerToolbar>
		 <Toolbar>
            <Title text="{i18n>custumorsList}"/>
            <ToolbarSpacer/>
            <SearchField width="50%" search=".onFilterCustomers"/>
         </Toolbar>
         
      </headerToolbar>
		<items>
			<ObjectListItem
				title="{customer>FirstName}  {customer>LastName}"
				number = "{customer>CustomerId}"
				type="Navigation"
				press=".onPress"
				> 
				
			</ObjectListItem>
		</items>
	</List>
</mvc:View> 